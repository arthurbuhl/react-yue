{
  "name": "react-yue",
  "version": "0.2.0",
  "description": "Render the view of GUI apps with yue and react.",
  "main": "index.js",
  "scripts": {
    "start": "node lib/do_space/index",
    "dev": "node scripts/dev",
    "build_lib": "babel src --out-dir lib -w",
    "build": "babel src --out-dir lib",
    "test": "jest"
  },
  "author": "oyyd <oyydoibh@gmail.com>",
  "files": [
    "lib",
    "README.md"
  ],
  "peerDependencies": {
    "gui": "0.4.3",
    "react": "^16.2.0"
  },
  "dependencies": {
    "react-reconciler": "^0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.35",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chokidar": "^2.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gui": "0.4.3",
    "jest": "^22.0.3",
    "react": "^16.2.0"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "7"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "testRegex": "lib\\/.*__tests__\\/.*\\.spec\\.js$"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "extends": "airbnb",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "arrow-parens": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "jsx-a11y/label-has-for": 0
    }
  }
}
